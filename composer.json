{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hekmatinasser/verta": "^1.12",
        "intervention/image": "^2.5",
        "kavenegar/php": "^1.2",
        "laravel/framework": "^8.40",
        "laravel/tinker": "^2.5",
        "morilog/jalali": "3.*",
        "project/common": "dev-master",
        "project/course": "dev-master",
        "project/discount": "dev-master",
        "project/front": "dev-master",
        "project/payment": "dev-master",
        "shetabit/payment": "^4.0",
        "spatie/laravel-permission": "^4.2"
    },
    "repositories": [
        {
            "type" : "path",
            "url" : "./modules/Common/"
        },
        {
            "type" : "path",
            "url" : "./modules/Front/"
        },
        {
            "type" : "path",
            "url" : "./modules/Payment/"
        },
        {
            "type" : "path",
            "url" : "./modules/Course/"
        },
        {
            "type" : "path",
            "url" : "./modules/Discount/"
        }
    ],
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "User\\": "modules/User/",
            "Dashboard\\": "modules/Dashboard/",
            "Category\\": "modules/Category/",
            "RolePermissions\\": "modules/RolePermissions/",
            "Media\\": "modules/Media/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
